package vo;

public class ReplyVO {
	private int rnum;
	private int bnum;
	private String writer;
	private String comment;
	private int ref;
	private String regdate;
	
	public int getRnum() {
		return rnum;
	}
	public int getBnum() {
		return bnum;
	}
	public String getWriter() {
		return writer;
	}
	public String getComment() {
		return comment;
	}
	public int getRef() {
		return ref;
	}
	public String getRegdate() {
		return regdate;
	}
	
	public ReplyVO(int bnum, String writer, String comment, int ref, String regdate) {
		super();
		this.bnum = bnum;
		this.writer = writer;
		this.comment = comment;
		this.ref = ref;
		this.regdate = regdate;
	}
}
